{
  "name": "@sggc/stripe-nestjs",
  "version": "1.0.0",
  "description": "A simple Stripe module for NestJS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "update:check": "ncu",
    "update:upgrade": "ncu -u",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "ts-node example/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alikarimii/stripe-nestjs.git"
  },
  "keywords": [
    "Nestjs",
    "Stripe",
    "Payment"
  ],
  "author": "Ali karimi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alikarimii/stripe-nestjs/issues"
  },
  "homepage": "https://github.com/alikarimii/stripe-nestjs#readme",
  "devDependencies": {
    "@nestjs/platform-express": "^10.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "express": "^4.18.2",
    "jest": "^29.6.2",
    "npm-check-updates": "^16.11.1",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "stripe": "^12.18.0"
  },
  "files": [
    "lib/**/*"
  ]
}